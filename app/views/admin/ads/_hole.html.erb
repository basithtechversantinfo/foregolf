<%= f.fields_for :holes do |hole| %>
    <div class="col-md-2 hole" id="hole_num_<%="#{hole.index + 1}"%>">
        <div class="col-md-12 zero-left-right-padd whole-num" style="color:green">
            <%="Hole #{hole.index + 1 }"%>
        </div>
        <%= render  'admin/holes/form', hole: hole ,hole_num: hole.index + 1 %>
    </div>
<% end %>